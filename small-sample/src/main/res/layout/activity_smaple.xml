<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.smallraw.library.smaple.SampleActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <Button
            android:id="@+id/read_calendar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="READ_CALENDAR" />
        <Button
            android:id="@+id/write_calendar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="WRITE_CALENDAR" />
        <Button
            android:id="@+id/camera"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="CAMERA" />
        <Button
            android:id="@+id/read_contacts"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="READ_CONTACTS" />
        <Button
            android:id="@+id/write_contacts"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="WRITE_CONTACTS" />
        <Button
            android:id="@+id/get_accounts"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="GET_ACCOUNTS" />
        <Button
            android:id="@+id/access_fine_location"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="ACCESS_FINE_LOCATION" />
        <Button
            android:id="@+id/access_coarse_location"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="ACCESS_COARSE_LOCATION" />
        <Button
            android:id="@+id/record_audio"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="RECORD_AUDIO" />
        <Button
            android:id="@+id/read_phone_state"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="READ_PHONE_STATE" />
        <Button
            android:id="@+id/call_phone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="CALL_PHONE" />
        <Button
            android:id="@+id/read_call_log"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="READ_CALL_LOG" />
        <Button
            android:id="@+id/write_call_log"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="WRITE_CALL_LOG" />
        <Button
            android:id="@+id/add_voicemail"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="ADD_VOICEMAIL" />
        <Button
            android:id="@+id/use_sip"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="USE_SIP" />
        <Button
            android:id="@+id/process_outgoing_calls"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="PROCESS_OUTGOING_CALLS" />
        <Button
            android:id="@+id/body_sensors"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="BODY_SENSORS" />
        <Button
            android:id="@+id/send_sms"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="SEND_SMS" />
        <Button
            android:id="@+id/receive_sms"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="RECEIVE_SMS" />
        <Button
            android:id="@+id/read_sms"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="READ_SMS" />
        <Button
            android:id="@+id/receive_wap_push"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="RECEIVE_WAP_PUSH" />
        <Button
            android:id="@+id/receive_mms"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="RECEIVE_MMS" />
        <Button
            android:id="@+id/read_external_storage"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="READ_EXTERNAL_STORAGE" />
        <Button
            android:id="@+id/write_external_storage"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="WRITE_EXTERNAL_STORAGE" />
    </LinearLayout>
</ScrollView>
